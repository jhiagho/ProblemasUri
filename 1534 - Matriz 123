#include <iostream>
using namespace std;


int main()
{
	int mat[80][80];
	int N;
	while(cin >> N)
{
	
	for (int i = 0; i < N; i++)
	{
		for (int j = 0; j < N; j++)
		{
			if(i == N - 1 - j){
				mat[i][j] = 2;
			}
			else if(i == j){
				mat[i][j] = 1;
			}
			else{
			mat[i][j] = 3;
			}
		}
	}
	
	for (int i = 0; i < N; i++)
	{
		for (int j = 0; j < N; j++)
		{
			cout << mat[i][j];
		}
		cout << "\n";
	}
	
}
	
	
	
	
	
	
	
	
	
	
	
}
